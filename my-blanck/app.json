{
  "expo": {
    "name": "my-blanck",
    "slug": "my-blanck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sonrisas.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/changito.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/changito.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/changito.png"
    },
    "plugins":
      "expo-splash-screen"
  }
}
